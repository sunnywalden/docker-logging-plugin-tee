{
  "description": "A docker logging plugin that send logs to multiple log drivers",
  "documentation": "https://github.com/buzztaiki/docker-logging-plugin-tee",
  "entrypoint": ["/usr/bin/docker-logging-plugin-tee"],
  "network": {
    "type": "bridge"
  },
  "interface": {
    "types": ["docker.logdriver/1.0"],
    "socket": "tee.sock"
  }
}


